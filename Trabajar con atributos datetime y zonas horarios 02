import pandas as pd

position = pd.read_csv('/datasets/position.csv')
position['timestamp'] = pd.to_datetime(
    position['timestamp'], format='%Y-%m-%dT%H:%M:%S')

dt_months = position['timestamp'].dt.month

print(dt_months.head(5))

# resultado :
# 0    2
# 1    2
# 2    2
# 3    2
# 4    2
# Name: timestamp, dtype: int64
